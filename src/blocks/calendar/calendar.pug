mixin calendar({ isOpen= 'false', range='true', lang, separator=' - ', dateFormat='dd.mm.yyyy', initDates= ''}={})
  .calendar.js-calendar(
    data-range=range,
    data-multiple-dates-separator=separator,
    data-language=lang,
    data-format=dateFormat,
    data-init-dates=initDates,
    data-is-open=isOpen,
  )