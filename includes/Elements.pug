extends ../template-head/main.pug

block variables
    - var pageTitle = 'Элементы'
    - var linkStyle = '../css/elements.css'
    - var linkStyle1 = '../css/searchRoom.css'
    - var linkStyle2 = '../css/Registration.css'
    - var linkStyle3 = '../css/roomDetails.css'
    - var linkStyle3 = '../css/range.css'



block header
    a(href="../index.html")
        div.logo
block content
    div.container-elements1
        div.elements-left
            div.elements-left__input
                div.elements-left__head-input
                    h3.elemets-left__title Text Field
                    p.option Default
                input.elements-left__input-element(placeholder="Email")
            div.elements-left__input
                div.elements-left__head-input
                    h3.elemets-left__title Text Field
                    p.option Hover / Focus
                input#input-el2.elements-left__input-element(placeholder="This is pretty awesome")
            div.elements-left__input
                div.search_room
                    div.residence
                        div.block3
                            h3.elemets-left__title Text Field
                            div.guests1
                                input.counte_guests(placeholder="Сколько Гостей" autocomplete="off" readonly="")
                                div.arrow
            div.elements-left__input
                h3.elements-left__title masked text field
                input.elements-left__input-element(placeholder="Email")
            div.elements-left__input
                div#el5.elements-left__input-element
                    div.elements-left__left-block
                        h3.elements-left__sub-title date Dropdown
                        div.search_room
                            div.residence
                                div.block_date
                                    div.block1
                                        div.date_arrival ДД.ММ.ГГГГ
                                            div.arrow
                                            include ../modules/calendar.pug
                    div.elements-left__left-block
                        h3.elements-left__sub-title date Dropdown
                        div.search_room
                            div.residence
                                div.block_date
                                    div.block2(style="margin-left:18px;")
                                        div.date_departure ДД.ММ.ГГГГ
                                            div.arrow
                                            include ../modules/calendar.pug
            div.elements-left__input
                h3.elements-left__title filter date dropdown
                div.content_search_room
                    div.data_arrival_in_otel
                        input(type="text" autocomplete="off" placeholder="19 авг - 23 авг" readonly="")
            div.elements-left__input
                h3.elements-left__title subscription text field
                    div.block5
                        div.email
                            input(placeholder="Email")

        div.elements-center
            div.elements-center__block-elements
                h3.elements-center__title Checkbox Buttons
                div.elements-center__checkboxs
                    div.order_in_house
                        div.checkbox_order
                            input#block_1.custom-checkbox(type="checkbox" name="block_1")
                            label(for="block_1") Можно курить
                    
                            input#block_2.custom-checkbox(type="checkbox" name="block_2")
                            label(for="block_2") Можно с питомцами
                        
                            input#block_3.custom-checkbox(type="checkbox" name="block_3" )
                            label(for="block_3") Можно пригласить гостей (до 10 человек)
            div.elements-center__block-elements
                h3.elements-center__title Radio Buttons
                div.elements-center__checkboxs
                    div.gender
                        input#block_checkbox_1.gender_checkbox(type="radio" name="block_checkbox")
                        label(for="block_checkbox_1") Мужчина
                        input#block_checkbox_2.gender_checkbox(type="radio" name="block_checkbox")
                        label(for="block_checkbox_2") Женщина
            div.elements-center__block-elements
                h3.elements-center__title Toggle
                div.elements-center__checkboxs
                    div.special_offer
                        input#block_checkbox_3.offer_checkbox(type="checkbox" name="block_checkbox" checked="checked")
                        label(for="block_checkbox_3") Получать спецпредложения
                    div.special_offer
                        input#block_checkbox_3.offer_checkbox(type="checkbox" name="block_checkbox")
                        label(for="block_checkbox_3") Получать спецпредложения
            div.elements-center__block-elements
                h3.elements-center__title like button
                div#checkbox-like.elements-center__checkboxs
                    div.heart.heart_1
                        div.fa_heart
                        span 12 
                    div.heart.heart_2
                        div.fa_heart.fa_heart2
                        span#span_heart 2 
            div.elements-center__block-elements
                h3.elements-center__title rate button
                div#checkbox-star.elements-center__checkboxs
                    div.star
                        div.stars#star1
                        div.stars#star2
                        div.stars#star3
                        div.stars#star4
                        div.stars#star5
                        
                    div.star
                        div.stars#star1
                        div.stars#star2
                        div.stars#star3
                        div.stars#star4
                        div.stars#star5
        div.elements-right
            div.elements-right__block-elements
                div.price_hotel
                    div.middle
                        div.container_price
                            div.price_title Range Slider
                            div.price
                                span#minValue
                                span ₽
                                span -
                                span#maxValue
                                span ₽
                        div.multi-range-slider
                            input#input-left(type="range" min="0" max="15000" value="1000")
                            input#input-right(type="range" min="0" max="15000" value="3000")
                            div.slider
                                div.track
                                div.range
                                div.thumb.left
                                div.thumb.right
            div.elements-right__block-elements
                h3.elements-right__title buttons
                div.elements-right__buttons.elements-right__buttons1
                    div.elements-right__btn1
                        div.if_have_account
                            a(href="#")
                                div.login
                                    p CLICK ME
                    div.elements-right__btn2
                        div.if_have_account
                            a(href="#")
                                div.login
                                    p CLICK ME
                div.elements-right__buttons.elements-right__buttons2
                    div.elements-right__btn1
                        div.if_have_account
                            a(href="#")
                                div.login
                                    p CLICK ME
                    div.elements-right__btn2
                        div.if_have_account
                            a(href="#")
                                div.login
                                    p CLICK ME
                div.elements-right__buttons.elements-right__buttons3
                    div.elements-right__btn1
                        div.if_have_account
                            a(href="#")
                                p CLICK ME
                    div.elements-right__btn2
                        div.if_have_account
                            a(href="#")
                                p CLICK ME
                div.elements-right__buttons.elements-right__buttons4
                    div.buy
                        p перейти к оплате
            div.elements-right__block-elements
                h3.elements-right__title pagination
                div.switch
                    div.container
                        div.circle.circle_1
                            p(style="color:#fff;")
                                b 1
                        div.circle.circle_2
                            p 2
                        div.circle.circle_3
                            p 3
                        div.circle.circle_4
                            p ...
                        div.circle.circle_5
                            p 15
                        div.circle.circle_6
                            i.fas.fa-arrow-right
                    p(style="font-size: 14px;line-height: 24px;margin:11px 0 0 0;") 1 – 12 из 100+ вариантов аренды
    div.con-elements2
        div.elements-left2
            div.elements-left2__drops.elements-left2__drop1
                div.elements-left2__head-drop
                    h3.elements-left2__title dropdown
                    p.elements-left2__option Default
                div.elements-left2__dropElmenet
                    div.conveniences1
                        input(placeholder="2 спальни, 2 кровати" autocomplete="off" readonly="")
            div.elements-left2__drops.elements-left2__drop2
                div.elements-left2__head-drop
                    h3.elements-left2__title dropdown
                    p.elements-left2__option expanded
                div.elements-left2__dropElmenet
                    div.conveniences
                        input#block_6.counte_conveniences(type="text" name="block_6" placeholder="2 спальни, 2 кровати" autocomplete="off" readonly="")
                    div.dropBlockCon.dropBlock
                        div.drop_block_item
                            div.drop_block_row
                                h3#dropItem.dropItem.dropItemCon1 спальни
                                button.circle_drop_item.circle_con_minus1(data-direction="minus") -
                                input#counterCon1(type="text" value="0")
                                button.circle_drop_item.circle_con_plus1(data-direction="plus") +
                            div.drop_block_row
                                h3#dropItem2.dropItem.dropItemCon2 кровати
                                button.circle_drop_item.circle_con_minus2(data-direction="minus") -
                                input#counterCon2(value="2")
                                button.circle_drop_item.circle_con_plus2(data-direction="plus") +
                            div.drop_block_row
                                h3#dropItem3.dropItem.dropItemCon3 ванные комнаты
                                button.circle_drop_item.circle_con_minus3(data-direction="minus") -
                                input#counterCon3(value="2")
                                button.circle_drop_item.circle_con_plus3(data-direction="plus") +
                            div.drop_function
                                button#clear.link_drop_function ОЧИСТИТЬ
                                button#apply.link_drop_function ПРИМЕНИТЬ
            div.elements-left2__drops.elements-left2__drop3
                div.elements-left2__dropElmenet
                    div.convenience1
                        span expandable checkbox list
                        div.arrow_convenience1
            div.elements-left2__drops.elements-left2__drop4
                div.elements-left2__dropElmenet
                    div.convenience
                        span expandable checkbox list
                        div.active.arrow_convenience
                        div.drop_block_convenience
                            div.drop_item_convenience
                                input#drop_1.convenience-checkbox(type="checkbox" name="drop_1")
                                label(for="drop_1") Завтрак

                                input#drop_2.convenience-checkbox(type="checkbox" name="drop_2" checked="checked")
                                label(for="drop_2") Письменный стол

                                input#drop_3.convenience-checkbox(type="checkbox" name="drop_3" checked="checked")
                                label(for="drop_3") Стул для кормления

                                input#drop_4.convenience-checkbox(type="checkbox" name="drop_4" checked="checked")
                                label(for="drop_4") Кроватка

                                inputdrop_5.convenience-checkbox(type="checkbox" name="drop_5")
                                label(for="drop_5") Телевизор

                                input#drop_6.convenience-checkbox(type="checkbox" name="drop_6")
                                label(for="drop_6") Шампунь
        div.elements-center2
            div.elements-center2__drops.elements-center2__drop1
                h3.elements-center2__title dropdown
                div.elements-center2__dropElmenet
                    div.guests
                        input.counte_guests(placeholder="Сколько Гостей" autocomplete="off" readonly="")
                        div.arrow
                    div.dropBlock
                        div.drop_block_item
                            div.drop_block_row
                                h3#dropItem1.dropItem Взрослые
                                div.db1
                                    button.circle_drop_item.circle_drop_minus1 -
                                    input#counter1(type="text" value="0")
                                    button.circle_drop_item.circle_drop_plus1 +
                            div.drop_block_row
                                h3#dropItem2.dropItem Дети
                                div.db2
                                    button.circle_drop_item.circle_drop_minus2 -
                                    input#counter2(value="0")
                                    button.circle_drop_item.circle_drop_plus2 +
                            div.drop_block_row
                                h3#dropItem3.dropItem Младенцы
                                div.db3
                                    button.circle_drop_item.circle_drop_minus3 -
                                    input#counter3(value="0")
                                    button.circle_drop_item.circle_drop_plus3 +
                            div.drop_function
                                button#apply.link_drop_function ПРИМЕНИТЬ
            div.elements-center2__drops.elements-center2__drop2
                h3.elements-center2__title rich Checkbox Buttons
                div.elements-center2__dropElmenet
                    div.availability
                        input#block_4.custom-checkbox(type="checkbox" name="block_4")
                        label(for="block_4")
                            span
                                b(style="color:#575870; font-size:14px;") Широкий коридор
                            p Ширина коридоров в номере не менее 91 см.
                        input#block_5.custom-checkbox(type="checkbox" name="block_5")
                        label(for="block_5")
                            b(style="color:#575870; font-size:14px;") Помощник для инвалидов
                            p На 1 этаже вас встретит специалист  и  проводит до номера.
        div.elements-right2
            div.elements-right2__drops.elements-right2__drop1
                h3.elements-right2__title dropdown
                div.guests
                    input.counte_guests(placeholder="Сколько Гостей" autocomplete="off" readonly="")
                    div.arrow
                div.dropBlock
                    div.drop_block_item
                        div.drop_block_row
                            h3#dropItem1.dropItem Взрослые
                            div.db1
                                button.circle_drop_item.circle_drop_minus1 -
                                input#counter1(type="text" value="2")
                                button.circle_drop_item.circle_drop_plus1 +
                        div.drop_block_row
                            h3#dropItem2.dropItem Дети
                            div.db2
                                button.circle_drop_item.circle_drop_minus2 -
                                input#counter2(value="1")
                                button.circle_drop_item.circle_drop_plus2 +
                        div.drop_block_row
                            h3#dropItem3.dropItem Младенцы
                            div.db3
                                button.circle_drop_item.circle_drop_minus3 -
                                input#counter3(value="0")
                                button.circle_drop_item.circle_drop_plus3 +
                        div.drop_function
                            button#clear.link_drop_function ОЧИСТИТЬ
                            button#apply.link_drop_function ПРИМЕНИТЬ
            div.elements-right2__drops.elements-right2__drop2
                h3.elements-right2__title bullet list
                div.order
                    ul.ul_order
                        li.li_order1 Нельзя с питомцами 
                        li.li_order2 Без вечеринок и мероприятий
                        li.li_order3
                            span Время прибытия — после 13:00
                                span а выезд до 12:00  
        

                

            


                



            

